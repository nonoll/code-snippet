{
  "name": "code-snippet_example_typescript",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "preinstall": "echo \"[Hello, @nonoll/webpack-typescript-boilerplate]\"",
    "install": "echo \"[NPM INSTALL DONE!]\"",
    "prebuild": "echo \"[RUN PREBUILD!]\" && npm run del:dist",
    "del:dist": "del-cli ./dist",
    "del:jsdoc": "del-cli ./docs",
    "dev:test": "jest --watch",
    "dev:e2e": "jest ./tests/__tests__/e2e.spec.ts",
    "dev:jsdoc": "nodemon --exec 'jsdoc -c jsdoc.json' --watch ./src/**/* -e js,ts,md",
    "dev:webpack": "cross-env TS_NODE_PROJECT=\"webpack.tsconfig.json\" webpack-dev-server --open --mode development --env.NODE_ENV=development --progress --watch",
    "dev": "echo \"[RUN DEV SERVER!]\" && npm run dev:webpack",
    "build:test": "jest",
    "build:jsdoc": "npm run del:jsdoc && jsdoc -c jsdoc.json",
    "build:webpack": "cross-env TS_NODE_PROJECT=\"webpack.tsconfig.json\" webpack --mode production --env.NODE_ENV=production",
    "build": "echo \"[RUN BUILD!]\" && npm run build:test && npm run build:jsdoc && npm run build:webpack"
  },
  "devDependencies": {
    "@nonoll/code-snippet": "../../../dist/code-snippet",
    "@nonoll/jsdoc": "https://github.com/nonoll/jsdoc#master",
    "@types/copy-webpack-plugin": "^6.0.0",
    "@types/html-webpack-plugin": "^3.2.3",
    "@types/jest": "^26.0.8",
    "@types/jquery": "^3.5.1",
    "@types/mini-css-extract-plugin": "^0.9.1",
    "@types/node": "^14.0.27",
    "@types/webpack": "^4.41.21",
    "@types/webpack-bundle-analyzer": "^3.8.0",
    "@types/webpack-dev-server": "^3.11.0",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "awesome-typescript-loader": "^5.2.1",
    "copy-webpack-plugin": "^6.0.3",
    "cross-env": "^7.0.2",
    "css-loader": "^4.1.0",
    "dart-sass": "^1.25.0",
    "del-cli": "^3.0.1",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "ink-docstrap": "^1.3.2",
    "jest": "^26.2.1",
    "mini-css-extract-plugin": "^0.9.0",
    "nodemon": "^2.0.4",
    "playwright": "^1.2.1",
    "prettier": "^2.0.5",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.2",
    "style-loader": "^1.2.1",
    "ts-jest": "^26.1.4",
    "ts-loader": "^8.0.1",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.7",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
